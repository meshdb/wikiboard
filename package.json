{
  "name": "wikiboard",
  "version": "0.0.0",
  "description": "A Vue.js project",
  "author": "Will Hilton <wmhilton@gmail.com>",
  "private": true,
  "scripts": {
    "http": "nodemon --exec babel-node http-server/server.js",
    "start": "npm run dev",
    "watchify": "watchify -vd -p browserify-hmr -p [ vueify/plugins/extract-css -o bundles/build.css ] -e src/main.js -o bundles/build.js",
    "serve": "http-server -o",
    "dev": "npm-run-all --parallel watchify serve",
    "lint": "eslint --ext .js,.vue src",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o bundles/build.css ] -e src/main.js > bundles/build.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js",
    "meshdb": "meshdb/src/index.js"
  },
  "dependencies": {
    "@wmhilton/login-with-github-vue-component": "0.0.5",
    "babel-cli": "^6.23.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "get-stream": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "level-browserify": "^1.1.0",
    "level-range": "0.0.0",
    "level-sublevel": "^6.6.1",
    "levelgraph": "^2.0.0",
    "lodash": "^4.17.4",
    "markdown-it": "^8.2.2",
    "openpgp": "^2.3.5",
    "pad": "^1.0.2",
    "pify": "^2.3.0",
    "s-ago": "^1.1.0",
    "semantic-ui-css": "^2.2.4",
    "shasum": "^1.0.2",
    "signalhub": "^4.8.0",
    "simplemde": "^1.11.2",
    "upath": "^1.0.0",
    "vue": "^2.0.1",
    "webrtc-swarm": "^2.8.0",
    "wrtc": "0.0.61"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^2.0.0",
    "envify": "^3.4.1",
    "eslint": "^3.3.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "http-server": "^0.9.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^2.3.0",
    "proxyquireify": "^3.0.1",
    "uglify-js": "^2.5.0",
    "vueify": "^9.0.0",
    "watchify": "^3.4.0"
  }
}
